{
    "name": "stefanopace/musement-forecast",
    "description": "Forecast service for Musement cities",
    "type": "project",
    "authors": [
        {
            "name": "Stefano Pace",
            "email": "stefanopace01@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*",
        "symfony/http-client": "^5.2",
        "symfony/console": "^5.2"
    },
    "config": {
        "platform": {
            "php": "7.4.16"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.85",
        "symplify/easy-coding-standard": "^9.2"
    }
}
